GET     /                    controllers.HomeController.index
GET     /api/v1/locations    controllers.VistorEventController.list(startDate: String, boundary: String, limit: Option[Int])
POST    /api/v1/locations    controllers.VistorEventController.create

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file        controllers.Assets.versioned(path="/public", file: Asset)

